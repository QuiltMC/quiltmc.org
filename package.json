{
	"name": "quiltmc-website",
	"version": "0.0.1",
	"description": "Website for the Quilt modloader",
	"packageManager": "pnpm@7.9.0",
	"scripts": {
		"dev": "astro dev",
		"start": "astro start",
		"build": "astro build && minhtml --collapse-boolean-attributes --collapse-whitespace --decode-entities --remove-comments --remove-empty-attributes --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-style-link-type-attributes --use-short-doctype --minify-css --minify-js --file-ext html --input-dir dist --output-dir dist",
		"preview": "astro preview"
	},
	"devDependencies": {
		"@astrojs/markdown-component": "^1.0.0",
		"@astrojs/mdx": "^0.8.1",
		"@astrojs/sitemap": "^1.0.0",
		"@creativebulma/bulma-divider": "^1.1.0",
		"@creativebulma/bulma-tooltip": "^1.2.0",
		"@types/luxon": "^3.0.0",
		"astro": "1.0.0",
		"astro-i18next": "1.0.0-beta.3",
		"bulma": "^0.9.4",
		"bulma-timeline": "^3.0.5",
		"fluent_conv": "^3.1.0",
		"github-slugger": "^1.4.0",
		"i18next": "^21.9.0",
		"i18next-fluent": "^2.0.0",
		"luxon": "^3.0.1",
		"minhtml": "^1.1.1",
		"remark-excerpt": "1.0.0-beta.1",
		"rollup": "^2.77.2",
		"sass": "^1.54.3",
		"stimulus": "^3.1.0",
		"typescript": "^4.7.4"
	}
