---
import TeamMember from "@parts/team/TeamMember.astro";
import { InferEntrySchema } from "astro:content";
import { CollectionEntry } from "astro:content";
import { getCollection } from "astro:content";


export interface Props {
	team: InferEntrySchema<"teamMembers">["teams"][0]
}

let { team } = Astro.props

const members = await getCollection("teamMembers", ({ data }) => {
	const stringTeams = data.teams // Still figuring out how to type this properly
	return stringTeams.includes(team)
})
---

{
	members.map((member) => (
		<TeamMember
			member={member}
		/>
	))
}
